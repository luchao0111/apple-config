<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns="http://www.appleframework.com/schema/config"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
        targetNamespace="http://www.appleframework.com/schema/config">

        <xsd:element name="annotation-driven" type="annotationDriven" />
        
        <xsd:complexType name="annotationDriven">
   			<xsd:attribute name="id" type="xsd:ID" />
     		<xsd:attribute name="name" type="xsd:string" />
     		<xsd:attribute name="location" type="xsd:string" />
     		<xsd:attribute name="loadRemote" type="xsd:boolean" />
     		<xsd:attribute name="eventListenerClass" type="xsd:string" />
        </xsd:complexType>

</xsd:schema>